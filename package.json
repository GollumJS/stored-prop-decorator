{
  "name": "vue-stored-prop-decorator",
  "version": "4.2.3",
  "description": "Add @Stored property annotation for vuex in Typscript mode. Replace property by setter and getter on store.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "esnext": "dist/esm/index.js",
  "typings": "./dist/typings/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rm -fr dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GollumJS/vue-stored-prop-decorator.git"
  },
  "keywords": [
    "vuex",
    "store",
    "prop",
    "property",
    "decorator",
    "typescript",
    "vuejs",
    "vue"
  ],
  "author": "Damien Duboeuf <smeagolworms4@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GollumJS/vue-stored-prop-decorator/issues"
  },
  "homepage": "https://github.com/GollumJS/vue-stored-prop-decorator#readme",
  "dependencies": {
    "vue": "^2.5",
    "vuex": "^3"
  },
  "devDependencies": {
    "typescript": "^4.6.3"
  },
  "files": [
    "src/**/*.ts",
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json",
    "tsconfig.esm.json"
  ]
}
